{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "no-mixed-operators": 0,
        "no-confusing-arrow": 0,
        "array-callback-return": 0,
        "no-param-reassign":0,
        "max-len": 0,
        "comma-dangle": 0,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "object-curly-newline": 0,
        "one-var-declaration-per-line": 0,
        "no-plusplus": 0,
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-console": 0,
        "one-var": 0
    }
}